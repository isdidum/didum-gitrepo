<?xml version="1.0" ?>
<configuration>
    <locator wkid="4326">http://tasks.arcgisonline.com/ArcGIS/rest/services/Locators/TA_Address_NA/GeocodeServer</locator>
    <excludeField>Zip4</excludeField>
    <minscore>40</minscore>
    <!-- by default every locator field is a text box, you can change one field to a dropdown
         by specifying it as a <listfield> and its values in <listvalues> -->
    <listfield>Country</listfield>
    <listvalues>Canada,USA</listvalues>
    <zoomscale>5000</zoomscale>
    <info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
</configuration>
