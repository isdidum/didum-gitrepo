<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx">
	
	<fx:Declarations>
		<!--file, new, mxml component is what's here 
			Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<!-- All skin requires a states property that include at -->
	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled"/>
	</s:states>
	
	<fx:Metadata>
		[HostComponent("spark.components.Application")]
	</fx:Metadata>
	
	<s:Rect horizontalCenter="0" width="750" height="100%" radiusX="10">
		<s:fill>
			<s:SolidColor color="#FFFBCF"/><!--lightyellow-->
		</s:fill>
		
		<s:stroke>
			<s:SolidColorStroke color="#AAAAAA"/>
		</s:stroke>
	</s:Rect>
	
	<s:Group id="contentGroup" top="20" horizontalCenter="0">
		
	</s:Group>
	
</s:SparkSkin>
